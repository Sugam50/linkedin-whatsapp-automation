# Generative AI (Google)
GEMINI_API_KEY=your_generative_api_key_here
# Optional: override default model (example: 'models/text-bison-001' or supported Gemini model)
GENERATIVE_MODEL=models/text-bison-001

# LinkedIn API (OAuth)
LINKEDIN_CLIENT_ID=your_linkedin_client_id
LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret
LINKEDIN_REDIRECT_URI=http://localhost:3000/auth/linkedin/callback

# Telegram Bot
TELEGRAM_BOT_TOKEN=123456:ABC-DEF-your-telegram-token
TELEGRAM_WEBHOOK_SECRET=your_webhook_secret_here
TELEGRAM_WEBHOOK_URL=https://test.com/auto/webhook

# Database (auto-created)
DB_PATH=./data/database.db
DATABASE_URL=postgresql://postgres:password@db.yourproject.supabase.co:5432/postgres

# Supabase (optional - for Storage)
SUPABASE_URL=https://yourproject.supabase.co
SUPABASE_SERVICE_KEY=your_supabase_service_role_key

# Images directory
IMAGES_DIR=./images

# App config
NODE_ENV=development
PORT=3000

